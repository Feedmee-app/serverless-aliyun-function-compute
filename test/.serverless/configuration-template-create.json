{
  "Resources": {
    "sls-storage-bucket": {
      "Type": "ALIYUN::OSS:Bucket",
      "Properties": {
        "BucketName": "sls-my-service",
        "Region": "cn-shanghai"
      }
    },
    "sls-function-service": {
      "Type": "ALIYUN::FC::Service",
      "Properties": {
        "name": "my-service-dev",
        "region": "cn-shanghai"
      }
    },
    "sls-fc-exec-role": {
      "Type": "ALIYUN::RAM::Role",
      "Properties": {
        "RoleName": "sls-my-service-dev-exec-role",
        "Description": "Allow Function Compute Service my-service-dev to access other services, generated by the Serverless framework",
        "AssumeRolePolicyDocument": {
          "Version": "1",
          "Statement": [
            {
              "Action": "sts:AssumeRole",
              "Effect": "Allow",
              "Principal": {
                "Service": [
                  "fc.aliyuncs.com"
                ]
              }
            }
          ]
        },
        "Policies": []
      }
    }
  }
}